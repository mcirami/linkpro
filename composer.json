{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "braintree/braintree_php": "^6.4",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^v5.2.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "^2.7",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "laravel/vapor-cli": "^1.31",
        "laravel/vapor-core": "^2.19",
        "laravel/vapor-ui": "^1.4",
        "laravelcollective/html": "^6.3",
        "mailchimp/marketing": "^3.0",
        "maxbanton/cwh": "^2.0",
        "predis/predis": "^1.1",
        "shopify/shopify-api": "^4.1",
        "signifly/laravel-shopify": "^1.2",
        "socialiteproviders/mailchimp": "^4.1",
        "socialiteproviders/shopify": "^4.1",
        "spatie/laravel-permission": "^5.8",
        "spiral/roadrunner": "^2.4",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2",
        "tcg/voyager": "1.6.*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.14",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
